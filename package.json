{
  "name": "code",
  "version": "1.0.0",
  "private": true,
  "config": {
    "env": "dev",
    "version-git-message": "Update App version to %s"
  },
  "scripts": {
    "app-version:config": "yarn version",
    "app-version:update": "yarn version --patch",
    "setup:android": "yarn --frozen-lockfile && jetify && yarn iwg:apply-identifier",
    "setup:ios": "yarn --frozen-lockfile && yarn copy:scandit && yarn pod:install && yarn iwg:apply-identifier",
    "setup:ios-debug": "yarn --frozen-lockfile && yarn copy:scandit && yarn pod:install-debug && yarn iwg:apply-identifier",
    "setup:full": "yarn --frozen-lockfile && jetify && yarn copy:scandit && yarn pod:install && yarn iwg:apply-identifier",
    "setup:full-debug": "yarn --frozen-lockfile && jetify && yarn copy:scandit && yarn pod:install-debug && yarn iwg:apply-identifier",
    "install:android": "yarn && jetify && yarn iwg:apply-identifier",
    "install:ios": "yarn && yarn copy:scandit && yarn pod:install && yarn iwg:apply-identifier",
    "install:ios-debug": "yarn && yarn copy:scandit && yarn pod:install-debug && yarn iwg:apply-identifier",
    "install:full": "yarn && jetify && yarn copy:scandit && yarn pod:install && yarn iwg:apply-identifier",
    "install:full-debug": "yarn && jetify && yarn copy:scandit && yarn pod:install-debug && yarn iwg:apply-identifier",
    "pod:install": "cd ios/ && pod repo update && export NO_FLIPPER=1 && pod install && unset NO_FLIPPER && cd ../",
    "pod:install-debug": "cd ios/ && pod repo update && pod install && cd ../",
    "pod:update": "cd ios/ && pod repo remove trunk && pod update && cd ../",
    "jetify": "jetify",
    "start": "node node-scripts/kill-metro.js && react-native start",
    "start:clear-cache": "yarn start --reset-cache",
    "ios": "unset PREFIX && ENVFILE=.env.$npm_package_config_env react-native run-ios",
    "ios:dev": "yarn config:env dev && yarn ios",
    "ios:sim": "yarn run ios --simulator=\"$npm_package_config_simulator\"",
    "ios:automation": "yarn ios:enable-sideloading && yarn config:env automation && yarn ios --configuration UAT.Release",
    "ios-edge:automation": "yarn ios:enable-sideloading && yarn config:env ios-edge-automation && yarn ios --configuration UAT.Release",
    "ios:enable-sideloading": "node node-scripts/enableIosSideloadingForTests.js",
    "android": "ENVFILE=.env.$npm_package_config_env react-native run-android",
    "android:dev": "adb reverse tcp:9900 tcp:9900 && yarn config:env dev && yarn android",
    "android:automation": "adb reverse tcp:5040 tcp:5040 && yarn config:env automation && yarn android:update-manifest && yarn android:bundle:release && yarn android:bundle:duplicates && yarn android --variant=release",
    "android-edge:automation": "adb reverse tcp:5040 tcp:5040 && yarn config:env android-edge-automation && yarn android:update-manifest && yarn android --variant=release",
    "android:apk": "cd android && ENVFILE=.env.$npm_package_config_env ./gradlew app:assembleRelease && cd ../",
    "android:aab": "cd android && ENVFILE=.env.$npm_package_config_env ./gradlew bundleRelease && cd ../",
    "android:run": "yarn start & yarn android",
    "device:shake": "adb shell input keyevent 82",
    "test": "BABEL_ENV=test jest",
    "javasdk": "/usr/libexec/java_home",
    "clean:javascript": "watchman watch-del-all && rm -rf node_modules && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/haste-* && rm -rf $TMPDIR/metro-*",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "clean:android-cache": "cd android && ./gradlew --stop && cd ../ && rm -rf ~/.gradle/caches/",
    "clean:ios": "rm -rf ~/Library/Developer/Xcode/DerivedData && rm -rf ios/Pods",
    "clean:all": "yarn clean:android && yarn clean:ios && yarn clean:javascript",
    "mock-server:start": "node node-scripts/kill-mockserver.js && node mock-server/start.js",
    "mock-server:stop": "pkill -f ctp-mock-server",
    "test:ios": "node_modules/.bin/wdio test/conf/wdio.ios.conf.js --exclude ./test/features/androidOnly/**/*.feature",
    "test:ios-sauce": "./test/automation-scripts/ios-sauce.sh",
    "test:android-sauce": "./test/automation-scripts/android-sauce.sh",
    "test:ios-wiremock": "node_modules/.bin/wdio test/conf/wdio.ios.conf.js",
    "env:update": "node node-scripts/updateAutomationEnvironment.js",
    "android:update-network-manifest": "node node-scripts/updateAndroidNetworkConfig.js",
    "wdio-tunnelid:android": "node node-scripts/sauceConnectAndroidIdentifier.js",
    "wdio-tunnelid:ios": "node node-scripts/sauceConnectIosIdentifier.js",
    "postinstall": "patch-package",
  },
